{
  "name": "@tsdl/express",
  "description": "TSDL express.js integration",
  "version": "0.1.9",
  "gitHead": "be0c5a1189afab2d9fc2889ffd81c95552cb195b",
  "author": "Jonathan Asplund <jonathan@asplund.net>",
  "license": "MIT",
  "scripts": {
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module es6 --outDir dist/esm",
    "build": "npm run build:cjs & npm run build:esm"
  },
  "types": "./dist/cjs/express/index.d.ts",
  "main": "./dist/cjs/express/index.js",
  "files": [
    "dist/cjs/express",
    "dist/esm/express"
  ],
  "exports": {
    ".": {
      "types": "./dist/cjs/express/index.d.ts",
      "import": "./dist/esm/express/index.js",
      "require": "./dist/cjs/express/index.js"
    },
    "./*": {
      "import": "./dist/esm/express/*.js",
      "require": "./dist/cjs/express/*.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asplunds/tsdl.git",
    "directory": "packages/express"
  },
  "peerDependencies": {
    "express": "^3 || ^4 || ^5"
  },
  "dependencies": {
    "@tsdl/core": "^0.1.8",
    "@tsdl/server": "^0.1.8"
  }
}
