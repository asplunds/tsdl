{
  "name": "tsdl",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "test": "vitest run",
    "lint": "eslint && prettier . -w",
    "release": "npx lerna publish --no-private",
    "prepare": "husky install && npx lerna link",
    "build": "npx lerna run --parallel build",
    "readme": "ts-node scripts/compileReadme.ts",
    "prune:dist": "find packages/**/* -name 'dist' -type d -prune -exec rm -rf '{}' +"
  },
  "private": true,
  "devDependencies": {
    "@tanstack/react-query": "^4.29.12",
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.0.1",
    "eslint": "^8.7.0",
    "express": "^4.18.2",
    "husky": "^8.0.0",
    "lerna": "6.0.1",
    "madge": "^6.0.0",
    "nx": "16.0.1",
    "nx-cloud": "latest",
    "prettier": "^2.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vitest": "^0.31.4"
  },
  "workspaces": [
    "packages/*",
    "tests/*",
    "playground/*",
    "meta/*"
  ],
  "author": "Jonathan Asplund <jonathan@asplund.net>",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0"
  }
}
